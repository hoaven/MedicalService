ul li{
    list-style: none;
}

a{
    color: #000;
}

.main-content{
    width: 100%;
    margin: 0 auto;
    padding-top: 50px;
}

.top-content{
    width: 75%;
    height: 450px;
    margin: 0 auto;
}

.topleft{
    /*width: 716px;*/
    width: 67%;
    height: 450px;
    margin-right: 30px;
    padding-left: 12px;
    float: left;
    border:3px solid #dbdbdb;
    border-top: 0px;
    box-shadow: 5px 2px 6px #393939;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    /*background: #ffffff;*/
}

.topleft img{
    margin-top: 10px;
}

.topleft .news{
    padding-top: 20px;
}

.topleft .news li{
    margin-bottom: 15px;
    /*width: 570px;*/
    width: 90%;
    height: 18px;
    background: url("../images/3.png") no-repeat;
}

.topleft .news li a.title{
    /*width: 370px;*/
    width: 68%;
    float: left;
    margin-left: 28px;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.topleft .news time{
    /*width: 150px;*/
    width: 23%;
    font-size: 13px;
    text-align: right;
    float: right;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.topleft .more{
    margin-top: -20px;
}

.topright{
    /*width: 320px;*/
    width: 30%;
    height: 450px;
    border-left: 2px dotted #dbdbdb;
    padding-left: 40px;
    padding-top: 50px;
    float: right;
}

.popstar{
    width: 85%;
    height: 290px;
    padding-top: 10px;
    box-shadow: 5px 2px 6px #898989;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    background: -moz-linear-gradient(top,#f1c1c1,#f7e6d7);
    background: -webkit-linear-gradient(top,#f1c1c1,#f7e6d7);
    background: -ms-linear-gradient(top,#f1c1c1,#f7e6d7);
    background: -o-linear-gradient(top,#90eef0,#22a3ff);
    background: linear-gradient(top,#90eef0,#22a3ff);
    filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr=#90eef0,endcolorstr=#22a3ff,gradientType=0);
}

.topright .text1{
    width: 180px;
    height: 40px;
    font-size: 18px;
    text-align: center;
    color: #fff;
}

.topright .text1 span,.bottomright .text1 span{
    display: block;
    width: 40px;
    height: 40px;
    float: left;
    background: -moz-linear-gradient(top,#90eef0,#22a3ff);
    background: -webkit-linear-gradient(top,#90eef0,#22a3ff);
    background: -ms-linear-gradient(top,#90eef0,#22a3ff);
    background: -o-linear-gradient(top,#90eef0,#22a3ff);
    background: linear-gradient(top,#90eef0,#22a3ff);
    filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr=#90eef0,endcolorstr=#22a3ff,gradientType=0);
    line-height: 2.2em;
    border-radius: 18px;
    -webkit-border-radius: 18px;
    -moz-border-radius: 18px;
}

.popstar ul,.bottomright ul{
    /*width: 230px;*/
    width: 98%;
    text-align: center;
    margin-left: -20px;
}

.popstar ul li,.bottomright ul li{
    margin-bottom: 18px;
}

.popstar ul li h3,.popstar ul li h3 a,.actstar ul li h3,.actstar ul li h3 a{
    color: #d4351e;
}

.bottom-content{
    width: 75%;
    height: 510px;
    margin: 0 auto;
    padding-top: 20px;
}

.bottomleft{
    width: 330px;
    /*width: 40%;*/
    height: 440px;
    float: left;
    background: #ffffff;
    border:3px solid #dbdbdb;
    box-shadow: 5px 2px 6px #393939;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
}

.bottomleft ul{
    margin-top: 20px;
}

.bottomleft ul li{
    width: 200px;
    height: 20px;
    margin-left: -15px;
    margin-bottom: 13px;
    background: url("../images/2.png") no-repeat;
}

.bottomleft ul li a.policy,.bottomcenter ul li a.policy{
    width: 230px;
    float: left;
    margin-left: 37px;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.more{
    float: right;
    margin-right: 20px;
}

.bottomcenter{
    width: 330px;
    height: 440px;
    float: left;
    margin-left: 55px;
    margin-right: 30px;
    padding-top: 40px;
    background: #ffffff;
    border:3px solid #dbdbdb;
    box-shadow: 5px 2px 6px #393939;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
}

.bottomcenter ul{
    margin-top: 10px;
}

.bottomcenter ul li{
    width: 200px;
    height: 20px;
    margin-left: -18px;
    margin-bottom: 13px;
    background: url("../images/6.png") no-repeat;
}

.bottomright{
    width: 320px;
    height: 430px;
    float: right;
    border-left: 2px dotted #dbdbdb;
    padding-top: 20px;
    padding-left: 40px;
}

.actstar{
    width: 85%;
    height: 290px;
    padding-top: 10px;
    box-shadow: 5px 2px 6px #898989;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    background: -moz-linear-gradient(top,#f1c1c1,#f7e6d7);
    background: -webkit-linear-gradient(top,#f1c1c1,#f7e6d7);
    background: -ms-linear-gradient(top,#f1c1c1,#f7e6d7);
    background: -o-linear-gradient(top,#f1c1c1,#f7e6d7);
    background: linear-gradient(top,#f1c1c1,#f7e6d7);
    filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr=#f1c1c1,endcolorstr=#f7e6d7,gradientType=0);
}

.bottomright .text1{
    width: 180px;
    height: 40px;
    margin-top: -30px;
    margin-left: 26px;
    text-align: center;
    color: #fff;
    font-size: 18px;
    line-height: 2.2em;
    float: right;
    margin-right: 20px;
}

.actstar{
    margin-top: 10px;
}



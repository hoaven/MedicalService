*{
    margin: 0;
    padding: 0;
}
body{
    background-color: #bde2f9;
}
[class*="col-"] {
    padding-top: 10px;
    padding-bottom: 10px;
}
#title{
    padding-left: 20px;
    font-weight: bold;
}
#main{
    border: 1px solid #6a6a6a;
}
#left{
    text-align: center;
    font-size: 20px;
    font-weight: bold;
}
#right{
    font-size: 18px;
    border-left: 1px solid #6a6a6a;
}
#tit{
    text-align: center;
}
.rig{
    margin-top: 30px;
    font-size: 15px;
}
#bot{
    border:3px solid #fff;
    font-size: 20px;
    font-weight: bold;
}
#text{
    border: 1px solid #fff;
    width: 300px;
    height: 150px;
    background-color: #bde2f9;
    font-size: 20px;
}
.btn{
    float: right;
    margin-left: 20px;
    margin-top: 150px;
}
footer{
    width: 100%;
    height: 120px;
    background: #9d9d9d;
    color: #ffffff;
    font-size: 15px;
    text-align: center;
    padding-top: 20px;
    margin-top: 50px;
}